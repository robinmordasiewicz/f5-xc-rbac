{
  "extensions": {
    "front-matter": {
      "enabled": true
    }
  },
  "plugins": {
    "line-length": {
      "enabled": false,
      "line_length": 120
    },
    "heading-style": {
      "enabled": true,
      "style": "atx"
    },
    "ul-style": {
      "enabled": true,
      "style": "dash"
    },
    "ol-prefix": {
      "enabled": false
    },
    "no-inline-html": {
      "enabled": false
    },
    "no-bare-urls": {
      "enabled": false
    },
    "first-line-heading": {
      "enabled": false
    },
    "commands-show-output": {
      "enabled": false
    },
    "no-emphasis-as-heading": {
      "enabled": false
    },
    "ul-indent": {
      "enabled": true,
      "indent": 4
    }
  }
}
